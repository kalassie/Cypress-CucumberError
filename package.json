{
  "name": "cypress-cucumber-error",
  "version": "1.0.0",
  "description": "fix project",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --spec cypress/integration/*.feature",
    "cypress:tags": "cypress run -e TAGS='@regression'",
    "cypress:report": "cypress run --reporter mochawesome && open ./mochawesome-report/mochawesome.html",
    "simple_build": "cd ../Cypress-CucumberError && npm install",
    "clean:report": "rimraf mochawesome-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kalassie/Cypress-CucumberError.git"
  },
  "keywords": [
    "cypress",
    "cucumber",
    "testing"
  ],
  "homepage": "https://github.com/kalassie/Cypress-CucumberError#readme",
  "Dependencies": {
    "cypress": "^4.4.1"
  },
  "devDependencies": {
    "cypress-cucumber-preprocessor": "^2.3.1",
    "eslint-plugin-cypress": "^2.10.3",
    "mocha": "^5.2.0",
    "mochawesome": "^3.1.1",
    "rimraf": "^2.6.2"
  },
  "author": "kalassie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kalassie/Cypress-CucumberError/issues"
  }
}
